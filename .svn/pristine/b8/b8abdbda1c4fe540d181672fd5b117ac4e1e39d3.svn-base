*{
    margin: 0;
    -webkit-box-sizing: border-box;
   -moz-box-sizing: border-box;
    box-sizing: border-box;
}
/*CSS Grid begin
=================================================*/
.container{
    width: 940px;
    margin: 0 auto;
/*    background-color: rgb(242,242,240);*/
}
.row{
    margin-bottom: 20px;
}
.row:after{
    display: block;
    content: '.';
    height: 0;
    visibility: hidden;
    clear: both;
}
[class*="span"]{
    float: left;
    display: inline;
    margin-right: 20px;
}
[class*="span"]:last-child{
    margin-right: 0;
    float: right;
}
.span1{width: 60px;}
.span2{width: 140px;}
.span3{width: 220px;}
.span4{width: 300px;}
.span5{width: 380px;}
.span6{width: 460px;}
.span7{width: 540px;}
.span8{width: 620px;}
.span9{width: 700px;}
.span10{width: 780px;}
.span11{width: 860px;}
.span12{width: 940px;}

.offset1{margin-left: 60px;}
.offset2{margin-left: 140px;}
.offset3{margin-left: 220px;}
.offset4{margin-left: 300px;}
.offset5{margin-left: 380px;}
.offset6{margin-left: 460px;}
.offset7{margin-left: 540px;}
.offset8{margin-left: 620px;}
.offset9{margin-left: 700px;}
.offset10{margin-left: 780px;}
.offset11{margin-left: 860px;}
.offset12{margin-left: 940px;}
/* CSS Grid end
=================================================*/
.header {
    position: fixed;
    width: 940px;
    height: 197px;
    background: rgb(242,242,240) url(../img/Time%20Control.PNG) no-repeat center;
    background-size: 25% auto;
	top: 0;
	border-radius: 15px;
/*    position: relative;*/
}


.header-nav {
    position: absolute;
    margin: 0 auto;
    bottom: 0;
    height: 40px;
    text-align: center;
}
.title1{
    position: absolute;
    top: 40px;
    left: 30px;
}
.title2{
    position: absolute;
    top: 40px;
    right: 40px;
}
.header h1{
    font-size: 62px;
    color: #0088cc;
    text-shadow: #fff 10px 10px 8px;
}
.menu{
    cursor: pointer;
    position: relative;
    height: 30px;
    line-height: 30px;
    color: #ffffff;
    text-shadow: 0 -1px 0 rgba(0, 0, 0, 0.25);
    background-color: #5bb75b;
    background-image: -moz-linear-gradient(top, #62c462, #51a351);
    background-image: -webkit-gradient(linear, 0 0, 0 100%, from(#62c462), to(#51a351));
    background-image: -webkit-linear-gradient(top, #62c462, #51a351);
    background-image: -o-linear-gradient(top, #62c462, #51a351);
    background-image: linear-gradient(to bottom, #62c462, #51a351);
    background-repeat: repeat-x;
    border-color: #51a351 #51a351 #387038;
    border-color: rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.1) rgba(0, 0, 0, 0.25);
    -webkit-border-radius: 3px;
    -moz-border-radius: 3px;
    border-radius: 3px;
    font-size: 16px;

}
.menu:hover{
    background-image: linear-gradient(to top, #62c462, #51a351);    
}
.menu a{
    display: block;
    text-decoration: none;
    color: #ffffff;
}
.current-menu::after{
    content: '';
    position: absolute;
    width: 0;
    height: 0;
    border-top: 10px solid #5bb75b;
    border-left: 10px solid transparent;
    border-right: 10px solid transparent;
    bottom: -10px;
    left: 60px;
    
}
.content{
        margin-top: 200px;
        border: solid 1px #ccc;
        padding: 10px;
        border-radius: 15px;
        background-color: rgb(242,242,240);
    } 
    .content label{
        display: inline-block;
        width: 175px;
        color: #2e6e9e;
    }
.content .row input[type="text"]{
    width: 300px;
    margin-left: 50px;
    height: 30px;
    color: #555;
    border: solid 1px #ccc;
    box-shadow: inset 0 1px 1px rgba(0,0,0,0.075);
    border-radius: 5px;
    padding: 4px 6px;
}
input[type="submit"]{
/*    margin-left: 100px;*/
    color: #fff;
    width: 87px;
    height: 30px;
    line-height: 15px;
    display: inline-block;
    text-shadow: 0 -1px 0 rgba(0,0,0,0.25);
    border-radius: 5px;
    background-image: linear-gradient(to bottom,#62c462,#51a351);
    background-repeat: repeat-x repeat-y;
    border-color: rgba(0,0,0,0.1) rgba(0,0,0,0.1) rgba(0,0,0,0.25);
} 
.content .row select{    
    border-radius: 5px;
    height: 30px;
    margin-left: 50px;
    width: 300px;
}

#element_to_pop_up {
    background-color: #fff;
    border-radius: 10px 10px 10px 10px;
    box-shadow: 0 0 25px 5px #999;
    color: #111;
    display: none;
    min-width: 600px;
    min-height: 500px;
    padding: 25px;
}
    .b-close {
        color: #fff;
        display: block;
        position: absolute;
        background: #dd4b39;
        border: 3px solid #dd4b39;
        border-radius: 5px;
        left: 576px;
        top: -20px;
        width: 26px;
        line-height: 25px;
        padding-left: 5px;
        cursor: pointer;
        margin: 10px;
    }
.tabs{
    margin: 0 auto;
    width: 400px;
    border-bottom: solid 1px #000;
}
    .tabs li{
        float: left;
        margin-left: 10px;
        box-sizing: border-box;
    }
    .current-tabs{
        border: solid 1px red;
    }
    .tabs:after{
        content: '';
        clear: both;
        display: block;
        height: 0;
    }
